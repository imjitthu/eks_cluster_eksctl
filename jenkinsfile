pipeline {

    agent any

    stages {
        stage ('Creating Eks Cluster') {
            steps {
                sh '''
                eksctl create cluster -f cluster.yml
                '''
            }
        }
        stage ('Deleting Eks Cluster') {
            steps {
                sh '''
                eksctl delete cluster -f clustern.yml
                '''
            }
        }
    }
}

//eksctl create cluster --name roboshop --region us-east-1 --managed -node-zones us-east-1d, us-east-1f --zones us-east-1d, us-east-1f